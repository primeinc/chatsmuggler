{
  "name": "chatsmuggler",
  "version": "0.1.1",
  "description": "Export conversations from ChatGPT, Gemini, Grok, Claude, and DeepSeek.",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@10.22.0",
  "engines": {
    "node": "22.22.0",
    "pnpm": "10.22.0"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && pnpm run attest",
    "attest": "tsx scripts/attest-artifacts.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "audit": "pnpm audit --audit-level=high",
    "prepare": "husky"
  },
  "devDependencies": {
    "@microsoft/eslint-plugin-sdl": "1.1.0",
    "@types/chrome": "0.1.37",
    "@types/node": "25.3.0",
    "@typescript-eslint/eslint-plugin": "8.56.0",
    "@typescript-eslint/parser": "8.56.0",
    "typescript-eslint": "8.56.0",
    "eslint": "10.0.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-no-secrets": "2.2.2",
    "eslint-plugin-security": "4.0.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "tsx": "4.21.0",
    "vitest": "4.0.18"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --check"
    ],
    "scripts/**/*.ts": [
      "prettier --check"
    ],
    "*.{json,mjs,md}": [
      "prettier --check"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [],
    "overrides": {
      "minimatch": "^10.2.1"
    }
  }
}
